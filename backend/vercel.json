{
    "version": 2,
    "framework": null,
    "name": "technical-test-api",
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js",
        "methods": ["GET", "POST", "PUT"]
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
}